{
  "name": "sails-js",
  "version": "0.1.4",
  "description": "Parser and typescript code generator from Sails IDL files",
  "main": "lib/index.js",
  "preferGlobal": true,
  "type": "module",
  "license": "GPL-3.0",
  "author": "Gear Technologies",
  "bugs": {
    "url": "https://github.com/gear-tech/sails/issues"
  },
  "homepage": "https://github.com/gear-tech/sails/tree/master/js#readme",
  "repository": {
    "type": "git",
    "url": "git+https://githib.com/gear-tech/sails.git"
  },
  "keywords": [
    "gear",
    "sails"
  ],
  "bin": {
    "sails-js": "./app.js"
  },
  "exports": {
    "./cjs/package.json": "./cjs/package.json",
    "./cjs/*": "./cjs/*.js",
    ".": {
      "import": "./index.js",
      "require": "./cjs/index.js",
      "types": "./index.d.ts"
    }
  },
  "scripts": {
    "prebuild": "rm -rf lib",
    "pretest": "yarn build && node test/modify-import.js",
    "build": "rm -rf lib && rollup --config rollup.config.js && node compress-parser.js",
    "test": "yarn node --no-warnings --experimental-vm-modules $(yarn bin jest) --detectOpenHandles"
  },
  "peerDependencies": {
    "@gear-js/api": "^0.38.1",
    "@polkadot/api": "^11.0.1",
    "@polkadot/types": "^11.0.1"
  },
  "devDependencies": {
    "@gear-js/api": "^0.38.1",
    "@polkadot/api": "^11.0.1",
    "@polkadot/types": "^11.0.1",
    "@rollup/plugin-commonjs": "25.0.8",
    "@rollup/plugin-node-resolve": "15.2.3",
    "@rollup/plugin-typescript": "11.1.6",
    "@types/jest": "29.5.12",
    "@types/node": "20.14.2",
    "babel-jest": "29.7.0",
    "jest": "29.7.0",
    "rollup": "4.18.0",
    "rollup-plugin-peer-deps-external": "2.2.4",
    "rollup-plugin-typescript2": "0.36.0",
    "ts-jest": "29.1.4",
    "ts-node": "10.9.2",
    "typescript": "5.4.5"
  },
  "dependencies": {
    "commander": "12.1.0"
  }
}
