
!@sails: 0.9.2

service PingPongService {
    functions {
        Ping(payload: PingPongPayload) -> PingPongPayload;
    }
    types {
        enum PingPongPayload {
            Start(ActorId),
            Ping,
            Pong,
            Finished,
        }
    }
}

program PingPong {
    constructors {
        NewForBench();
    }
    services {
        PingPongService,
    }
}
