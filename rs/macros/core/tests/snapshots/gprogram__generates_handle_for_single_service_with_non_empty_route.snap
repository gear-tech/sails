            let mut __input = input;
            let input = String::decode(&mut __input)
                .unwrap_or_else(|_| {
                    if input.len() <= 8 {
                        format!("0x{}", sails_rs::hex::encode(input))
                    } else {
                        format!(
                            "0x{}..{}", sails_rs::hex::encode(& input[..4]),
                            sails_rs::hex::encode(& input[input.len() - 4..])
