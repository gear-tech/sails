        let (interface_id, route_id, entry_id, payload) = sails_message
            .try_match_interfaces(INTERFACE_IDS)
            .expect("Failed to find matching service");
        match route_id {
            _ => gstd::unknown_input_panic("Unknown route_id", &[route_id]),
        }
    }
