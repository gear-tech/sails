        + sails_rs::meta::count_base_services::<MyService>()
        + sails_rs::meta::count_base_services::<MyService>();
    const INTERFACE_IDS: &'static [(sails_rs::meta::InterfaceId, u8)] = &sails_rs::meta::interface_ids::<
        SERVICES_COUNT,
    >(
        &[
            sails_rs::meta::BaseServiceMeta::new::<MyService>(""),
            sails_rs::meta::BaseServiceMeta::new::<MyService>(""),
        ],
    );
