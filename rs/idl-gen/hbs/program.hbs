program {{name}} {
    constructors {
        {{#each ctors}}
        {{#each docs}}
        /// {{{this}}}
        {{/each}}
        {{name}}({{#each args}}{{#if @index}}, {{/if}}{{name}}: {{{lookup @root.program.concrete_names type}}}{{/each}});
        {{#unless @last}}

        {{/unless}}
        {{/each}}
    }
    {{#if services}}

    services {
        {{#each services}}
        {{this}},
        {{/each}}
    }
    {{/if}}
    {{#if types}}

    types {
{{#each types}}
{{#if this.enum}}
        enum {{this.enum.own_name}} {
        {{#each this.enum.fields}}
            {{own_name}}{{~#if fields}}
            {{~#if fields.[0].name}} {
                {{#each fields}}
                {{name}}: {{{type_name}}},
                {{/each}}
            }
            {{~else~}}
            ({{#each fields}}{{{type_name}}}{{#unless @last}}, {{/unless}}{{/each}})
            {{~/if}}{{~/if}},
        {{/each}}
        }
{{else}}
    {{#if this.struct.fields}}
        {{#if this.struct.fields.[0].name}}
        struct {{this.struct.own_name}} {
            {{#each this.struct.fields}}
            {{name}}: {{{type_name}}},
            {{/each}}
        }
        {{else}}
        struct {{this.struct.own_name}}({{#each this.struct.fields}}{{{type_name}}}{{#unless @last}}, {{/unless}}{{/each}});
        {{/if}}
    {{else}}
        struct {{this.struct.own_name}};
    {{/if}}
{{/if}}
{{#unless @last}}

{{/unless}}
{{/each}}
    }
    {{/if}}
}
