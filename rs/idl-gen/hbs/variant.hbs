{{#each docs}}
/// {{{this}}}
{{/each}}
type {{{lookup @root/type_names id}}} = enum {
{{#each variants}}
  {{#each docs}}
  /// {{{this}}}
  {{/each}}
  {{name}}
  {{~#if fields.[1]}}: struct {
    {{#each fields~}}
    {{#each docs}}
    /// {{{this}}}
    {{/each}}
    {{#if name}}{{name}}: {{/if~}}{{{lookup @root/type_names type}}},
    {{/each}}
  } {{~else}}
  {{~#if fields.[0]}}: {{#with fields.[0]}} {{~#if name~}} struct {
    {{#each docs}}
    /// {{{this}}}
    {{/each}}
    {{name}}: {{{lookup @root/type_names type}}}
  }{{else~}}
    {{{lookup @root/type_names type}}}
  {{~/if}}
  {{~/with~}}
  {{~/if~}}
  {{~/if~}}
  ,
{{/each}}
};